// MotorPH LinkedList Sample

class InventoryNode {
    int productId;
    String brand;
    String model;
    int quantity;
    InventoryNode next;

    InventoryNode(int productId, String brand, String model, int quantity) {
        this.productId = productId;
        this.brand = brand;
        this.model = model;
        this.quantity = quantity;
        this.next = null;
    }
}

public class linkedListSample {

    public static void main(String[] args) {

        // Create head node
        InventoryNode head = new InventoryNode(1001, "Honda", "Click 125", 5);

        // Add more nodes
        head.next = new InventoryNode(1002, "Yamaha", "NMAX", 3);
        head.next.next = new InventoryNode(1003, "Suzuki", "Raider", 2);

        System.out.println("MotorPH Inventory Linked List");

        // Display linked list
        InventoryNode current = head;
        while (current != null) {
            System.out.println(
                "Product ID " + current.productId +
                " Brand " + current.brand +
                " Model " + current.model +
                " Quantity " + current.quantity
            );
            current = current.next;
        }
    }
}
